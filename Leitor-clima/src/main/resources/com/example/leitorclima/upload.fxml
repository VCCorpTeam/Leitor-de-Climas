<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="77.0" prefWidth="398.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.leitorclima.GUI.UploadController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox alignment="CENTER" layoutX="30.0" layoutY="121.0" />
    <HBox alignment="CENTER_RIGHT" spacing="10">
        <TextField fx:id="txtPathFile" disable="true" editable="false" prefHeight="25.0" prefWidth="318.0">
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor>
        </TextField>
        <Button fx:id="btnSelectFile" mnemonicParsing="false" text="..." />
    </HBox>
    <HBox alignment="CENTER">
        <Button fx:id="btnUploadFile" text="Upload" />
    </HBox>
   <HBox alignment="CENTER" layoutX="30.0" layoutY="75.0" />
</VBox>
